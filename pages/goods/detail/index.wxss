.top{
    height:750rpx;
}
.top image{
    width:100%;
    height:750rpx;
}
.main-con{
    position: relative;
    padding:30rpx;
    margin-top:-140rpx;
}
.goods-price{
    color:#bd151d;
    font-size: 38rpx;
    line-height: 54rpx;
}
.collect{
    width:56rpx;
    height:52rpx;
    background: url(data:image/jpg;base64,) 0 0 no-repeat;
    background-size: 100%;
}
.collect.on{
    background: url(data:image/jpg;base64,) 0 0 no-repeat;
    background-size: 100%;
}
.goods-name{
    line-height: 58rpx;
    font-size: 38rpx;
}
.goods-tips {
    line-height: 56rpx;
    padding-bottom:10rpx;
    font-size: 32rpx;
    color:#bd151d;
    border-bottom:2rpx solid #f6f6f6;
}
.line-item{
    padding-top:10rpx;
    background: #fff;
    line-height: 64rpx;
    font-size: 34rpx;
    margin-bottom:-10rpx;
}
.line-item.arrow{
    position:relative;
}
.line-item.arrow:before{
     position:absolute; 
    content:"";
    right:0rpx;
    top:50%;
    width:15rpx;
    height:15rpx;
    border-top:3rpx solid #666;
    border-right:3rpx solid #666;
    transform: rotate(45deg) translate(-50%);
}
.line-item .lab{
    float:left;
    line-height: 64rpx;
    height: 64rpx;
}
.hr28{
    height: 120rpx;
}
.footer{
    position: fixed;
    width:100%;
    bottom:0;
    height: 120rpx;
    font-size: 40rpx;
    text-align: center;
    line-height: 120rpx;
    color: #fff;
}
.footer .b1{
    background: #725a38;
}
.footer .b2{
    background: #bd151d;
}
.detail-con{
    padding: 30rpx;
}
.detail-con .detail-tit{
    font-size: 40rpx;
    line-height: 56rpx;
}

.popup .title{
    position:relative;
    text-align: center;
    line-height: 100rpx;
    border-bottom:1px solid #eee;
    font-size: 28rpx;
    font-weight: 600;
}
.popup .title .close{
    position:absolute;
    right:32rpx;
    top:39rpx;
    width:22rpx;
    height:22rpx;
}
.popup .con{
    width:100%;
    position: fixed;
    bottom:0;
    z-index: 10;
    background:  #fff;
    -webkit-transition-duration: .3s;
    transition-duration: .3s;
}
.popup.popup-enter .con,
.popup.popup-leave .con{
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0);
}
.popup.popup-enter .mark{
    opacity: 0;
}
.scroll{
    height:600rpx;
}
.add-btn{
    position:relative;
    text-align: center;
    line-height: 100rpx;
    font-size: 32rpx;
    font-weight: 600;
    color:#fff;
    background: #bd151d;
}
.mui-number {
    position: relative;
    width:196rpx;
    height: 48rpx;
    text-align: center;
    line-height: 48rpx;
    letter-spacing: 0;
}

.mui-number.disabled {
    width: 6.1rem;
    height: 1.5rem;
    color: #fff;
    background-color: #aaa;
}

.mui-number .decrease, .mui-number .increase {
    position: relative;
    float: right;
    display: block;
    width:48rpx;
    height: 48rpx;
    background-color: #fff;
    color: #5f646e;
    border:2rpx solid #e0e0e0;
    border-radius: 3rpx;
}

.mui-number .decrease {
    float: left;
}

.mui-number .decrease:before {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    content: '';
    width: 22rpx;
    height: 2rpx;
    background-color: #626262;
}

.mui-number .increase:before {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    content: '';
    width: 22rpx;
    height: 2rpx;
    background-color: #626262;
}

.mui-number .increase:after {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    content: '';
    width: 2rpx;
    height: 22rpx;
    background-color: #626262;
}

.mui-number .num {
    float: left;
    width: 100rpx;
    height: 48rpx;
    line-height: 48rpx;
    border: none;
    text-align: center;
    color: #000;
    font-size: 22rpx;
}
.sku-con {
    padding-top:100rpx;
    text-align: center;
}
.sku-con .img{
    width:340rpx;
    height:340rpx;
}
.sku-con .price{
    font-size: 32rpx;
    font-weight: 700;
    color:#bd151d;
    line-height: 86rpx;
}
.operate{
    padding:30rpx;
}