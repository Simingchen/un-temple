.item-cart {
    position: relative;
    padding-top: 27rpx;
    padding-bottom: 27rpx;
    border-top:2rpx solid #f4f4f4;
    background-color: #fff;
}

.item-cart .check-container {
    float: left;
    width: 29rpx;
    height:29rpx;
    margin-top: 60rpx;
    margin-left: 0.7rem;
    margin-right:32rpx;
    box-sizing: border-box;
}

.shopCart-page .item-cart .check-container .check-state {
    display: inline-block;
    width: 2.0rem;
    height: 1.0rem;
    margin-left: 0.25rem;
    text-align: center;
    line-height: 1.0rem;
    color: #fff;
    border-radius: 0.15rem;
}

.shopCart-page .item-cart .check-container .check-state.failure {
    background-color: #aaa;
}

.shopCart-page .item-cart .check-failure {
    float: left;
    width: 2.0rem;
    padding: 0.25rem 0.5rem;
    margin-left: -0.5rem;
    color: #fff;
    background: #aaa;
    font-size: 0.6rem;
    border-radius: 3px;
}

.shopCart-page .item-cart .name-card {
