.panel{
    padding:30rpx;
}
.top-bar{
    text-align: center;
}
.top-bar image{
    width:88rpx;
    height:70rpx;
    background: #999;
}
.top-bar .txt{
    color:#666;
    line-height: 60rpx;
    font-size: 30rpx;
}
.top-bar .num{
    font-size: 34rpx;
    font-weight: 700;
}
.positon-tab{
    width:514rpx;
    margin:0 auto;
    text-align: center;
    font-size: 34rpx;
    line-height: 77rpx;
    margin-bottom:40rpx;
}
.positon-tab .flex-item.on .txt{
    padding-bottom:21rpx;
    color:#c7a769;
    border-bottom:4rpx solid #c7a769;
}
.position-list .item{
    float:left;
    width:88rpx;
    height:88rpx;
    margin-right: 24rpx;
    margin-bottom:24rpx;
    text-align: center;
    line-height: 88rpx;
    border:1rpx solid #e0e0e0;
    border-radius: 6rpx;
    font-size: 32rpx;
}
.position-list .item:nth-child(6n){
    margin-right: 0;
}
.position-list .toggle{
    position: relative;
    text-align: center;
}
.position-list .toggle text{
    padding-left:32rpx;
    background:url(data:image/jpg;base64,) 0 center no-repeat;
    background-size: 24rpx 14rpx!important;
}
.position-list .toggle text.on{
    background: url(data:image/jpg;base64,) 0 center no-repeat;
}
.lamp-list{
    position: relative;
    height:2832rpx;
}
.scroll{
    width:100%;
    height:2832rpx;
}
.lamp-list .lift{
    position: absolute;
    width:40rpx;
    padding:40rpx 0;
    text-align: center;
    color:#fff;
    background: #bfbfbf;
    border-radius:40rpx;
}
.lamp-list .lift .item{
    margin-bottom:130rpx;
}
.lamps {
    padding-left:60rpx;
}
.lamps .item{
    float:left;
    width:100rpx;
    height:140rpx;
    margin-bottom:20rpx;
    margin-right: 30rpx;
    text-align: center;
}
.lamps .item image{
    width:100rpx;
    height:80rpx;
    background: #999;
}
.lamps .item .no{
    line-height: 44rpx;
    font-size: 20rpx;
}
.hr28{
    height: 120rpx;
}
.footer{
    position: fixed;
    width:100%;
    bottom:0;
    height: 100rpx;
    font-size: 36rpx;
    line-height: 100rpx;
    color: #fff;
    text-align: center;
}
.footer .b2{
    width:100%;
    text-align: center;
    background: #bd151d;
}