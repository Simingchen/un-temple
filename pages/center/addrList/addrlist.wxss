.addrList-con .form-checkbox {
    border-radius: 100%;
    display: inline-block;
    vertical-align: middle;
    margin-right:15rpx;
    width: 30rpx;
    height: 30rpx;
    background-image: url(data:image/jpg;base64,);
    background-repeat: no-repeat;
    background-size: 100%;
}

.addrList-con .form-checkbox.checked {
    background-image: url(data:image/png;base64,);
}

.addrList-con .addNew {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    text-align: center;
    line-height: 100rpx;
    color: #fff;
    background:#c7a769;
    z-index: 5;
    border: 0;
    margin: 0 auto;
    border-radius: 0;
}

.hr22 {
    height: 100rpx;
    line-height: 100rpx;
}

page {
    color:#000;
}

.list-item {
    position:relative;
    min-height: 215rpx;
    margin-bottom: 10rpx;
    background: #fff;
    padding: 10rpx 33rpx 0 40rpx;
}
.addr-detail{
    font-size: 34rpx;
    line-height: 42rpx;
    color:#999;
}
.receiver-box{
    padding-right:57rpx;
    margin-top:20rpx;
